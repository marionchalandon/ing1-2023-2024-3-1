    }
    // Placer le pion au centre du tableau
    cadre[positionY][positionX] = 'S';

    while (1) {

        system("cls"); // Effacer l'écran avec la bibliotheque windows

        // Afficher le tableau car boucle
        for (int i = 0; i < 10; i++) {
            for (int j = 0; j < 20; j++) {
                printf("%c ", cadre[i][j]);
